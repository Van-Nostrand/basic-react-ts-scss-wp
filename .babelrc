{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "module-resolver",
      {
        "root": "./src",
        "extensions": [".js", ".jsx", ".css", ".json", ".svg"],
        "alias": {
          "@": "./src"
        }
      }
    ]
  ]
}
